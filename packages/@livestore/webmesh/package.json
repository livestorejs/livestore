{
  "name": "@livestore/webmesh",
  "version": "0.3.2-dev.7",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/mod.ts",
    "./websocket-server": "./src/websocket-server.ts"
  },
  "dependencies": {
    "@livestore/utils": "workspace:*"
  },
  "devDependencies": {
    "@livestore/utils-dev": "workspace:*",
    "vitest": "catalog:"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/mod.d.ts",
        "bun": "./src/mod.ts",
        "default": "./dist/mod.js"
      },
      "./websocket-server": {
        "types": "./dist/websocket-server.d.ts",
        "bun": "./src/websocket-server.ts",
        "default": "./dist/websocket-server.js"
      }
    }
  },
  "scripts": {
    "test": "vitest"
  }
}
