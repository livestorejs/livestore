{
  "name": "@livestore/livestore",
  "version": "0.3.2-dev.5",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/mod.ts",
    "./internal": "./src/internal/mod.ts",
    "./internal/testing-utils": "./src/utils/tests/mod.ts",
    "./effect": "./src/effect/mod.ts"
  },
  "dependencies": {
    "@livestore/common": "workspace:*",
    "@livestore/utils": "workspace:*",
    "@opentelemetry/api": "1.9.0"
  },
  "devDependencies": {
    "@livestore/adapter-web": "workspace:*",
    "@livestore/utils-dev": "workspace:*",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "catalog:"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "./internal": {
        "types": "./dist/internal/mod.d.ts",
        "default": "./dist/internal/mod.js"
      },
      "./internal/testing-utils": {
        "types": "./dist/utils/tests/mod.d.ts",
        "default": "./dist/utils/tests/mod.js"
      },
      "./effect": {
        "types": "./dist/effect/mod.d.ts",
        "default": "./dist/effect/mod.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest"
  }
}
