{
  "name": "@livestore/livestore",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./internal/testing-utils": {
      "types": "./dist/utils/tests/mod.d.ts",
      "default": "./dist/utils/tests/mod.js"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "default": "./dist/effect/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "testing-utils": [
        "./dist/utils/tests/mod.d.ts"
      ],
      "effect": [
        "./dist/effect/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@livestore/common": "workspace:*",
    "@livestore/db-schema": "workspace:*",
    "@livestore/utils": "workspace:*",
    "@opentelemetry/api": "^1.9.0"
  },
  "devDependencies": {
    "@livestore/web": "workspace:*",
    "@opentelemetry/sdk-trace-base": "1.27.0",
    "jsdom": "^25.0.1",
    "typescript": "5.5.4",
    "vite": "5.4.10",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "graphql": "16.x"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
