{
  "name": "@livestore/livestore",
  "version": "0.0.57",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "default": "./dist/effect/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "./dist/react/index.d.ts"
      ],
      "effect": [
        "./dist/effect/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest && REACT_STRICT_MODE=1 vitest"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@livestore/common": "workspace:*",
    "@livestore/utils": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "effect-db-schema": "workspace:*"
  },
  "devDependencies": {
    "@livestore/web": "workspace:*",
    "@opentelemetry/sdk-trace-base": "1.26.0",
    "@tauri-apps/api": "^1.6.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "jsdom": "^24.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "5.5.4",
    "vite": "5.4.3",
    "vitest": "^2.0.4"
  },
  "peerDependencies": {
    "graphql": "16.x",
    "react": "^18"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
