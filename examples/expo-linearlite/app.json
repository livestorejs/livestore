{
  "expo": {
    "name": "expo-linearlite",
    "slug": "linearlite-app",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "linearlite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.betoatexpo.linearlite",
      "splash": {
        "image": "./src/assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FAFAFA",
        "dark": {
          "image": "./src/assets/images/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#18181B"
        }
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.betoatexpo.linearlite",
      "splash": {
        "image": "./src/assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FAFAFA",
        "dark": {
          "image": "./src/assets/images/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#18181B"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-asset", "expo-build-properties", "expo-font", "expo-sqlite", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "eas": {
        "projectId": "fa9bf858-2af3-4d4f-9658-4b2422d9bafc"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "betoatexpo",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fa9bf858-2af3-4d4f-9658-4b2422d9bafc"
    }
  }
}
